        </xs:sequence>
        <xs:element minOccurs="0" ref="OrderID"/>
        <xs:element ref="Rarity"/>
        <xs:element minOccurs="0" ref="TeamColor"/>
        <xs:element minOccurs="0" ref="BroadcasterEnabled"/>
        <xs:sequence minOccurs="0">
          <xs:element ref="HairLt_Swap"/>
          <xs:element ref="Hair_Swap"/>

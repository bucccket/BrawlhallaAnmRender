          <xs:element ref="Weapon_Swap"/>
          <xs:element ref="WeaponDk_Swap"/>
          <xs:element ref="WeaponAcc_Swap"/>
        </xs:sequence>
        <xs:sequence minOccurs="0">
          <xs:element ref="ExcludeOpponentTeamColor"/>
          <xs:element ref="FallbackOpponentTeamColor"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="FallbackMyTeamColor"/>
        <xs:element minOccurs="0" ref="IndicatorColor"/>
      </xs:sequence>
      <xs:attribute name="ColorSchemeName" use="required" type="xs:NMTOKEN"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ColorSchemeID" type="xs:integer"/>
  <xs:element name="DefaultUnlocked" type="xs:NMTOKEN"/>
  <xs:element name="DisplayNameKey" type="xs:string"/>
  <xs:element name="IconName" type="xs:NMTOKEN"/>
  <xs:element name="IconFileName" type="xs:NCName"/>
  <xs:element name="UniversalIconName" type="xs:NMTOKEN"/>
  <xs:element name="UniversalIconFileName" type="xs:NCName"/>
  <xs:element name="OrderID" type="xs:integer"/>

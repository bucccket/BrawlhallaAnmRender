          <xs:element ref="Body2Acc_Swap"/>
          <xs:element ref="SpecialVL_Swap"/>
          <xs:element ref="SpecialLt_Swap"/>
          <xs:element ref="Special_Swap"/>
          <xs:element ref="SpecialDk_Swap"/>
          <xs:element ref="SpecialVD_Swap"/>
          <xs:element ref="SpecialAcc_Swap"/>
        </xs:sequence>
        <xs:sequence minOccurs="0">
          <xs:element ref="HandsLt_Swap"/>
          <xs:element ref="HandsDk_Swap"/>
          <xs:element ref="HandsSkinLt_Swap"/>
          <xs:element ref="HandsSkinDk_Swap"/>
        </xs:sequence>
        <xs:sequence minOccurs="0">
          <xs:element ref="ClothVL_Swap"/>

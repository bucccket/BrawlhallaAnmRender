<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="ColorSchemeTypes">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="ColorSchemeType"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ColorSchemeType">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ColorSchemeID"/>
        <xs:element ref="DefaultUnlocked"/>
